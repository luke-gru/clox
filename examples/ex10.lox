/**
Testing break/continue
*/
print "while test (1-7):";

var a = 1;
while (a < 10) {
  if (a == 8) {
    break;
  }
  print a;
  a = a + 1;
}

print "continue test: (1-7)";

var a = 0;
while (a < 10) {
  a = a + 1;
  if (a > 7) {
    continue;
  }
  print a;
}

