requireScript("testunit");

class MyTests < TestUnit {
  testPass() {
    this.assertEq(1, 1);
  }
  testFail() {
    this.assertEq(1, 2);
  }
  testMatch() {
    this.assertMatch("hi", %"hi");
  }
  testNoMatch() {
    this.assertMatch("hi", %"bye");
  }
}
MyTests.runTests(outputReport: false);
print MyTests.successes();
print MyTests.failures();

__END__
-- expect: --
2
2
